package metodos;

public class EstudoSobrecardaDeMetodos {
 public static void main(String[] args) {
	System.out.println("Iniciou no Canal..:  " + LigarTV());
	System.out.println("Mudou o canal..:  " + LigarTV(2));
	System.out.println("Iniciou no Canal..:  " + LigarTV(5, true));
}
 
 public static int LigarTV(){
	 int canal = 5;
	 return canal;
 }
 
 public static int LigarTV(int c){
	 int canal = c;
	 return canal;
 }
 
 public static int LigarTV(int c, boolean ligada){
	 if (ligada == true) {
	 int canal = c;
	 return canal;
	 }else{
		 ligada = true;
		 int canal = 0;
		 return canal;
	 }
 }
 
 
}
